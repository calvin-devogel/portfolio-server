  "query": "\n        SELECT\n            post_id,\n            title,\n            slug,\n            content,\n            excerpt,\n            author,\n            published,\n            created_at,\n            updated_at\n        FROM blog_posts\n        WHERE\n            (NOT $1 OR published = true)\n            AND ($2::text IS NULL OR slug = $2)\n        ORDER BY created_at DESC\n        LIMIT $3 OFFSET $4",
